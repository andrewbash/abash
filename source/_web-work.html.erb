<section class="feed container" id="work">
	<div class="container__inner">
		<h3 class="sectionTitle">
			Web<br>
			<strong>Work</strong>
		</h3>
		<% data.webWork.first(5).each do |webWork| %>
			<section class="feed__info row">
				<%= image_tag lorem.image('300x300'), :class => "column one-half" %>
				<div class="column one-half">
					<h4 class="info__title"><%= webWork.title %></h4>
					<div class="info_copy"><%= webWork.desc %></div>
					<%= link_to "See Demo", webWork.link, :class => "button" %>
				</div>
			</section>
		<% end %>
	</div>
</section>